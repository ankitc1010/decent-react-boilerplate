		"react-router": "^4.3.1",
		"react-router-dom": "^4.3.1"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.4",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"clean-webpack-plugin": "^0.1.19",
		"concurrently": "^3.5.1",
		"css-loader": "^0.28.11",
		"extract-text-webpack-plugin": "^4.0.0-beta.0",
