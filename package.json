{
	"name": "reactboiler",
	"version": "1.0.0",
	"description": "A basic boilerPlate for React",
	"main": "index.js",
	"scripts": {
		"start": "node server.js",
		"test": "jest",
		"devClient": "webpack-dev-server --mode development --open",
		"devServer": "node-dev server.js",
		"dev": "concurrently \"npm run devClient\" \"npm run devServer\"",
		"prod": "webpack --mode production",
		"coverage": "jest --coverage"
	},
	"keywords": [
		"boilerplate",
		"react",
		"es6",
		"webpack4"
	],
	"author": "Ankit Chauhan",
	"license": "ISC",
